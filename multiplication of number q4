// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,a,f;
    printf("enter the number which needs the multiplication table : ");
    scanf("%d",&a);
    printf("\n enter the no of multiples needed : " );
    scanf("%d",&n);
    for(int i=0;i<=n;i++){
        f=a*i;
        printf("\n%d x %d = %d",a,i,f);
    }
}
